    "AuthQueueLimit": 0,
    "UploadPermitLimit": 10,
    "UploadWindowInSeconds": 60,
    "UploadQueueLimit": 2,
    "ProjectPermitLimit": 50,
    "ProjectWindowInSeconds": 60,
    "ProjectQueueLimit": 5,
    "PublicPermitLimit": 200,
    "PublicWindowInSeconds": 60,
    "PublicQueueLimit": 20
  }
}
