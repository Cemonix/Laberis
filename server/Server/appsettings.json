{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "GlobalPermitLimit": 100,
    "GlobalWindowInSeconds": 60,
