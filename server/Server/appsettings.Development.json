{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:7231"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Authentication": {
    "UseFakeUser": true
  },
  "RateLimiting": {
    "GlobalPermitLimit": 1000,
    "GlobalWindowInSeconds": 60,
    "GlobalQueueLimit": 50,
    "AuthPermitLimit": 20,
    "AuthWindowInSeconds": 60,
    "AuthQueueLimit": 5,
    "UploadPermitLimit": 50,
    "UploadWindowInSeconds": 60,
    "UploadQueueLimit": 10,
    "ProjectPermitLimit": 200,
    "ProjectWindowInSeconds": 60,
    "ProjectQueueLimit": 20,
    "PublicPermitLimit": 500,
    "PublicWindowInSeconds": 60,
    "PublicQueueLimit": 50
  }
}
