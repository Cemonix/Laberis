# Example environment variables for a Laberis frontend
VITE_API_BASE_URL=?
VITE_APP_NAME=Laberis
VITE_APP_VERSION=0.1.0

# Logging Configuration
# Available levels: debug, info, warn, error
VITE_LOG_LEVEL=debug

# Enable logging in production builds (default: false)
VITE_LOG_IN_PRODUCTION=false

# Enable timestamps in log messages (default: true)
VITE_LOG_TIMESTAMPS=true

# Enable service prefixes in log messages (default: true)
VITE_LOG_SERVICE_PREFIX=true

# Auto-login in development mode (default: true)
VITE_AUTO_LOGIN_DEV=true